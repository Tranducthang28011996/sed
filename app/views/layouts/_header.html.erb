<header>
  <nav class ="round">
    <div id="links">
      <ul>
        <li><%= link_to "Home", root_path %></li>
        <% if current_user %>
          <li><%= link_to "Users", users_path %></li>
        <% end %>
      </ul>
    </div>
    <div id="current_user">
      <% if current_user %>
        Logged in as <%= current_user.name %>. <%= link_to "Log Out", logout_path %>
      <% else %>
        <%= link_to "Log In", login_path %>
      <% end %>
    </div>
    <div class="clear"></div>
  </nav>
</header>
